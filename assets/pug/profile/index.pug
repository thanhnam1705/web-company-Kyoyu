                span
                  span
                    | TEL.
                    a(href='tel:078-857-5663') 078-857-5663
                    br
                    | FAX.078-587-5788
              .outline-map
                a.map-link.is-second.trans.sm(href='https://goo.gl/maps/puysfge8Bso7tUUi9' target='_blank' rel='noopener')
                  img.image-common(src=imgSP+'icon_map_01.svg' width='54' height='69' alt='map' loading='lazy')
                iframe.md(src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3280.488114654939!2d135.253280876386!3d34.69286658363199!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x60008d991ddd03b1%3A0x2f955a1da84aacc9!2z5pel5pys44CB44CSNjU4LTAwMzMg5YW15bqr55yM56We5oi45biC5p2x54GY5Yy65ZCR5rSL55S66KW_77yS5LiB55uu77yRIOOCseOCpOODkuODsw!5e0!3m2!1sja!2s!4v1684863870814!5m2!1sja!2s' width='600' height='450' loading='lazy' referrerpolicy='no-referrer-when-downgrade')
